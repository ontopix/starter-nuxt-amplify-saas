{
  "name": "@starter-nuxt-amplify-saas/amplify",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "exports": {
    ".": "./nuxt.config.ts",
    "./server/utils/amplify": "./server/utils/amplify.ts"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "sandbox:generate-outputs": "ampx generate outputs --stack $SANDBOX_STACK_NAME",
    "sandbox:generate-graphql-client-code": "ampx generate graphql-client-code --stack $SANDBOX_STACK_NAME --out utils/graphql",
    "generate-outputs": "ampx generate outputs --branch $AWS_BRANCH --app-id $BACKEND_APP_ID",
    "generate-graphql-client-code": "ampx generate graphql-client-code --branch $AWS_BRANCH --app-id $BACKEND_APP_ID --out utils/graphql"
  },
  "dependencies": {
    "aws-amplify": "^6.8.4",
    "@aws-amplify/core": "^6.1.0",
    "@aws-amplify/api-graphql": "^4.7.19"
  },
  "devDependencies": {
    "@starter-nuxt-amplify-saas/backend": "workspace:*",
    "@nuxt/eslint": "latest",
    "eslint": "^9.31.0",
    "nuxt": "^4.0.0",
    "typescript": "^5.8.3",
    "vue": "latest"
  }
}
